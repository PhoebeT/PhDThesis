journal = {Nature materials},
number = {4},
pages = {332--336},
pmid = {23353629},
publisher = {Nature Publishing Group},
title = {{Ultrafast spin transport as key to femtosecond demagnetization.}},
url = {http://dx.doi.org/10.1038/nmat3546{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/23353629},
volume = {12},
year = {2013}
}
