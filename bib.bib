@article{Wustenberg2011,
abstract = {Ultrafast demagnetization upon excitation with intense laser pulses has been observed for a variety of ferromagnetic materials. In half-metals, long demagnetization times are expected due to the band gap for one spin direction. We have investigated the theoretically half-metallic Heusler alloy Co 2Cr 0.6Fe 0.4Al (CCFA), using the time-resolved magnetooptical Kerr effect. A demagnetization time in the range of typical transition metal ferromagnets with lower spin polarization has been found, while magnetization recovery proceeds on a slower time scale. The results are discussed in the context of recent models and experimental results. We propose that for Heusler alloys the initial stage of recovery could be a better measure for the spin polarization than the demagnetization time. A simple estimate leads to a bulk spin polarization value of 0.86 for CCFA. {\textcopyright} 2011 WILEY-VCH Verlag GmbH {\&} Co. KGaA, Weinheim.},
author = {W{\"{u}}stenberg, Jan Peter and Steil, Daniel and Alebrand, Sabine and Roth, Tobias and Aeschlimann, Martin and Cinchetti, Mirko},
doi = {10.1002/pssb.201147087},
file = {:C$\backslash$:/Users/phoeb/Documents/KMGroup Research/KM Group Research/Papers/W-stenberg{\_}et{\_}al-2011-physica{\_}status{\_}solidi{\_}{\%}28b{\%}29.pdf:pdf},
issn = {03701972},
journal = {Physica Status Solidi (B) Basic Research},
keywords = {Half-metals,Heusler alloys,Spin polarization,Ultrafast magnetization dynamics},
number = {10},
pages = {2330--2337},
title = {{Ultrafast magnetization dynamics in the half-metallic Heusler alloy Co2Cr0.6Fe0.4Al}},
volume = {248},
year = {2011}
}
