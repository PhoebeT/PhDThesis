isbn = {0163-1829},
issn = {01631829},
journal = {Phys. Rev.},
number = {3B},
pages = {B864--B871},
pmid = {14995397},
title = {{The Inhomogeneous Electron Gas}},
url = {http://link.aps.org/doi/10.1103/PhysRev.136.B864{\%}5Cnhttp://journals.aps.org/pr/abstract/10.1103/PhysRev.136.B864},
volume = {136},
year = {1964}
}
@article{Illg2013,
abstract = {Despite intensive research, the underlying mechanisms for ultrafast demagnetization after laser irradiation in transition metals are still not understood. We discuss the possible processes which have been suggested in order to explain the ultrafast demagnetization within several hundreds of femtoseconds and argue that the spin angular momentum has to go to the lattice in the end. Based on this argument, we consider spin-flip electron-phonon scatterings. The demagnetization time tau(M) and the demagnetization rate dM/dt due to spin-flip electron-phonon scattering is calculated for fcc Ni and bcc Fe. Thereby, the electronic states and phononic states are calculated ab initio. We find that the demagnetization rates for fcc Ni and bcc Fe are too small to explain experimental demagnetization rates, which is in agreement with earlier publications. In addition, the demagnetization rates for band structures with reduced exchange splitting are calculated, however, also these demagnetization rates are too small. Finally, the phase space for scattering processes which is related to the maximum possible demagnetization is estimated for band structures with ground-state exchange splitting and with reduced exchange splitting. The maximum possible demagnetization is too small for bcc Fe and fcc Co but not necessarily for fcc Ni. We suggest to include magnons and to consider independent combinations of spin-flip electron-phonon and spin-flip electron-magnon scattering processes as a possible explanation for the ultrafast demagnetization.},
author = {Illg, Christian and Haag, Michael and F{\"{a}}hnle, Manfred},
doi = {10.1103/PhysRevB.88.214404},
file = {:C$\backslash$:/Users/phoeb/Documents/KMGroup Research/KM Group Research/Papers/PhysRevB.88.214404.pdf:pdf},
isbn = {9783319077420},
issn = {10980121},
journal = {Physical Review B - Condensed Matter and Materials Physics},
number = {21},
pages = {1--11},
title = {{Ultrafast demagnetization after laser irradiation in transition metals: Ab initio calculations of the spin-flip electron-phonon scattering with reduced exchange splitting}},
volume = {88},
year = {2013}
}
