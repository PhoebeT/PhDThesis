@article{Koopmans2005,
abstract = {We present a microscopic model that successfully explains the ultrafast equilibration of magnetic order in ferromagnetic metals at a time scale tau(M) of only a few hundred femtoseconds after pulsed laser excitation. It is found that tau(M) can be directly related to the so-called Gilbert damping factor sigma that describes damping of GHz precessional motion of the magnetization vector. Independent of the spin-scattering mechanism, an appealingly simple equation relating the two key parameters via the Curie temperature T(C) is derived, tau(M) approximately c(0)h/k(B)T(C)sigma, with h and k(B) the Planck and Boltzmann constants, respectively, and the prefactor c(0) approximately 1/4). We argue that phonon-mediated spin-flip scattering may contribute significantly to the sub-ps response.},
author = {Koopmans, B. and Ruigrok, J. J.M. and {Dalla Longa}, F. and {De Jonge}, W. J.M.},
doi = {10.1103/PhysRevLett.95.267207},
file = {:C$\backslash$:/Users/phoeb/Documents/KMGroup Research/KM Group Research/Papers/PhysRevLett.95.267207.pdf:pdf},
issn = {00319007},
journal = {Physical Review Letters},
number = {26},
pages = {1--4},
pmid = {16486397},
title = {{Unifying ultrafast magnetization dynamics}},
volume = {95},
year = {2005}
}
@article{Koopmans2010,
abstract = {Pulsed-laser-induced quenching of ferromagnetic order has intrigued researchers since pioneering works in the 1990s. It was reported that demagnetization in gadolinium proceeds within 100 ps, but three orders of magnitude faster in ferromagnetic transition metals such as nickel. Here we show that a model based on electron-phonon-mediated spin-flip scattering explains both timescales on equal footing. Our interpretation is supported by ab initio estimates of the spin-flip scattering probability, and experimental fluence dependencies are shown to agree perfectly with predictions. A phase diagram is constructed in which two classes of laser-induced magnetization dynamics can be distinguished, where the ratio of the Curie temperature to the atomic magnetic moment turns out to have a crucial role. We conclude that the ultrafast magnetization dynamics can be well described disregarding highly excited electronic states, merely considering the thermalized electron system.},
author = {Koopmans, B and Malinowski, G and {Dalla Longa}, F and Steiauf, D and F{\"{a}}hnle, M and Roth, T and Cinchetti, M and Aeschlimann, M},
doi = {10.1038/nmat2593},
file = {:C$\backslash$:/Users/phoeb/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Koopmans et al. - 2010 - Explaining the paradoxical diversity of ultrafast laser-induced demagnetization.pdf:pdf},
isbn = {1476-1122},
issn = {1476-1122},
journal = {Nature materials},
number = {3},
pages = {259--265},
pmid = {20010830},
publisher = {Nature Publishing Group},
title = {{Explaining the paradoxical diversity of ultrafast laser-induced demagnetization.}},
